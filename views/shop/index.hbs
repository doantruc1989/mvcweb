<div class="container" style="margin-top: 200px;">
    <div class="col-lg-7 mx-auto text-center mb-6" style="margin-bottom: 50px;">
        <h1 class="fs-3 fs-lg-5 lh-sm mb-3" style="color: white;">Hot Hot Hot!!!</h1>
    </div>

    <div class="row">
        {{#each products}}
        <div class="col-md-4">
            <div class="card mb-4 shadow-sm" style="height: 500px;">
                <a href="/product/{{_id}}">
                    <img class="bd-placeholder-img card-img-top" width="100%" height="350" src="{{imagePath}}">
                </a>
                <div class="card-body">
                    <a href="/product/{{_id}}">
                        <h5>{{title}}</h5>
                    </a>
                    <h6>{{description}}</h6>
                    <p class="card-text"></p>
                    <div class="d-flex justify-content-between align-items-center">
                        <div class="btn-group">
                            <a class="btn btn-sm btn-outline-secondary" href="/product/{{_id}}">Detail</a>
                        </div>

                        <div class="btn-group">
                            <form action="/add-to-cart/{{_id}}" method="get">
                                <button type="submit" class="btn btn-sm btn-outline-secondary">Cart</button>

                            </form>
                        </div>

                        <small class="text-muted text-right">
                            <s></s>
                            <b>${{price}}</b>
                        </small>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>

